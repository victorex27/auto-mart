{
  "name": "auto-mart",
  "version": "1.0.0",
  "description": "Online marketplace for automobiles of diverse makes, model or body type",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "connect-multiparty": "^2.2.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-validator": "^5.3.1",
    "file-type": "^11.1.0",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2",
    "travis": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "lint": "eslint",
    "start": "cross-env NODE_ENV=production babel-node src/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/server.js",
    "test": "cross-env NODE_ENV=test mocha --exit test  --require @babel/register --timeout 50000",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text ./node_modules/mocha/bin/_mocha test",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorex27/auto-mart.git"
  },
  "author": "Obikobe Amaobi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victorex27/auto-mart/issues"
  },
  "homepage": "https://github.com/victorex27/auto-mart#readme"
}
